{% extends 'contacts/layout.html.twig' %}

{% block body %}
    {{ parent() }}
    <h3>List All User Contacts</h3>
    <div id="yellow-wrapper">
        <div class="list-group">
            {% for contact in contacts %}
                <a href="{{ path('useraccount_add_by_contact',{'id': contact.id}) }}" class="list-group-item">
                    {{ contact }}
                    <b>Created At: <b>{{ contact.created|date('Y-m-d H:i:s') }}</b></b>
                </a>
            {% endfor %}
        </div>
    </div>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}